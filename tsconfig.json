{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "lib": ["dom", "esnext"],
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "jsx"
  },
  "include": ["src"]
}
